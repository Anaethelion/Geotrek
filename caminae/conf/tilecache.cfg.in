[cache]
type=Disk
base=${options.cachedir}

[ign]
type=WMSLayer
layers=scan100,scan25
url=http://extranet.parcnational.fr/pnx/wms?
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}

[ortho]
type=WMSLayer
layers=ortho
url=http://extranet.parcnational.fr/pnx/wms?
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}
