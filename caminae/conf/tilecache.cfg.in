[cache]
type=Disk
base=${options.cachedir}

[${options.terrain_layername}]
type=WMSLayer
layers=${options.terrain_layers}
url=${options.wms_url}
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}

[${options.ortho_layername}]
type=WMSLayer
layers=${options.ortho_layers}
url=${options.wms_url}
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}
