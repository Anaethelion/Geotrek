[cache]
type=Disk
base=${options.cachedir}

[terrain]
type=WMSLayer
layers=${options.terrain_layers}
url=${options.wms_url}
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}

[ortho]
type=WMSLayer
layers=${options.ortho_layers}
url=${options.wms_url}
extension=jpg
tms_type=google
srs=EPSG:${options.srid}
bbox=${options.spatial_extent}
maxResolution=${options.max_resolution}
